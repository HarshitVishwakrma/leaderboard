{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/index.js",
        "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "https://userdashboard-theta.vercel.app" },
        { "key": "Access-Control-Allow-Credentials", "value": "true" }
      ]
      }
    ]
}